#!/bin/sh

echo "Jan 01 10:12:03 localhost kernel: [ 0000.000000] Initializing cgroup subsys cpu" | \
OUTPUT=`./plugin-loader missing 2>&1`;# 
if [ "$?" -ne "0" ]; then
    echo "$OUTPUT" | sed -re 's/(.*)/pl> \1/g' > missing.log;
    exit 0;
else
    echo "$OUTPUT" | sed -re 's/(.*)/pl> \1/g' > missing.log;
    exit 1;
fi
